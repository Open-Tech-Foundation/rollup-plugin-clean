{
  "name": "@open-tech-world/rollup-plugin-clean",
  "version": "0.12.1",
  "description": "A rollup plugin to remove files and folders using glob patterns.",
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": {
    "import": "./dist/index.esm.js",
    "require": "./dist/index.cjs"
  },
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "rollup -c -w",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "build": "rm -rf dist && rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-tech-world/rollup-plugin-clean.git"
  },
  "keywords": [
    "rollup",
    "plugin",
    "rollup-plugin",
    "clean",
    "clear",
    "delete",
    "remove",
    "files",
    "folders",
    "directory"
  ],
  "author": "Thanga Ganapathy <ganapathy888@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/open-tech-world/rollup-plugin-clean/issues"
  },
  "homepage": "https://github.com/open-tech-world/rollup-plugin-clean#readme",
  "devDependencies": {
    "@open-tech-world/node-glob": "^0.4.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/jest": "^27.0.0",
    "@types/node": "^16.4.14",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "rollup": "^2.56.2",
    "typescript": "^4.3.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@open-tech-world/node-rm": "^0.5.0"
  }
}
